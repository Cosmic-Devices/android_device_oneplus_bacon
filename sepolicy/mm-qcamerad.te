permissive mm-qcamerad;
permissive cameraserver;
hal_client_domain(mm-qcamerad, hal_graphics_allocator);
allow mm-qcamerad { surfaceflinger mediaserver cameraserver hal_graphics_allocator }:fd use;
allow mm-qcamerad vendor_camera_data_file:file { read write open getattr };
allow mm-qcamerad camera_socket:sock_file { create unlink write };
allow mm-qcamerad camera_data_file:sock_file { create unlink };
